function execute(s)
	if game:GetService("JointsService"):FindFirstChild("_FEBYPASS32") then
		local Execute=game:GetService("JointsService"):FindFirstChild("_FEBYPASS32"):FireServer(tostring(s)) -- execute
	else
		warn("Failed to loadstring: Error 404")
	end
end
