-- Danh s√°ch t·∫•t c·∫£ c√°c frame
local allFrames = {Ui, Ho, script, credit,code}

-- H√†m ·∫©n t·∫•t c·∫£ c√°c frame
local function hideAllFrames()
	for _, frame in pairs(allFrames) do
		frame.Visible = false
	end
end

-- H√†m hi·ªÉn th·ªã m·ªôt frame v√† ƒë·ªìng b·ªô v·ªã tr√≠
local function showFrame(targetFrame)
	-- T√¨m frame ƒëang hi·ªÉn th·ªã (frame c≈©)
	local currentPosition = nil
	for _, frame in pairs(allFrames) do
		if frame.Visible then
			currentPosition = frame.Position
			break
		end
	end

	-- ·∫®n t·∫•t c·∫£
	hideAllFrames()

	-- ƒê·ªìng b·ªô v·ªã tr√≠ n·∫øu c√≥ frame c≈©
	if currentPosition then
		targetFrame.Position = currentPosition
	end

	-- Hi·ªÉn th·ªã frame m·ªõi
	targetFrame.Visible = true
end

-- K·∫øt n·ªëi n√∫t
